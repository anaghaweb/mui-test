{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/Components/Navbar/BaseNavbar/MobileMenu/mobileMenu.tsx",
    "app/Components/Navbar/BaseNavbar/index.tsx",
    "app/LandingPage/page.js",
    "app/Components/Navbar/B2bNavbarDarkBlue/LargeScreenMenu/AtlassianSVG/atlassianSVGLight.jsx",
    "app/Components/Navbar/B2bNavbarLightBlue/LargeScreenMenu/AtlassianSVG/atlassianSVGLight.jsx",
    "app/Components/Navbar/B2bNavbarLightBlue/LargeScreenMenu/AtlassianSVG/atlassianSVGdark.jsx",
    "app/Components/Navbar/B2bNavbarDarkBlue/LargeScreenMenu/AtlassianSVG/atlassianSVGdark.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}